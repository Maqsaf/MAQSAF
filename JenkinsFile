pipeline{
    agent any
    tools{
        gradle 'GRADLE_LATEST'
        git 'Git'
    }
    stages{
        stage('Build'){
            steps{
                    // sh './gradlew clean build'
                
                    bat './gradlew.bat clean assembleDebug'
                
}
            
        }
        stage('Archive'){
            steps{
                archiveArtifacts artifacts: '**/*.apk', followSymlinks: false
            }
        }
    }
}
